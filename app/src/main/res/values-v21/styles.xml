<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">@color/colorprimary</item>
        <item name="colorPrimaryDark">@color/colorprimaydark</item>
        <item name="colorAccent">@color/coloraccent</item>
        <item name="drawerArrowStyle">@style/drawerarrowstyle</item>
        <item name="android:actionOverflowButtonStyle">@style/threedoticon</item>

        </style>
    <style name="drawerarrowstyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="spinBars">true</item>
        <item name="android:color">@color/whitebackgroud</item>
    </style>

<style name="threedoticon" parent="Base.Widget.AppCompat.ActionButton.Overflow">
<item name="android:src" >@drawable/textsize</item>

</style>
</resources>
